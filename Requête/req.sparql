// 1. Liste de tous les pays avec leur capitale et continent

PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX schema: <http://schema.org/>
PREFIX ex: <http://example.org/population/>

SELECT ?country ?capital ?continent
WHERE {
  ?country rdf:type ex:Country ;
           schema:capital ?capital ;
           schema:continent ?continent .
}
LIMIT 100


// 2. Trouver les pays d’Afrique avec une densité supérieure à 100 habitants/km²
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX schema: <http://schema.org/>
PREFIX ex: <http://example.org/population/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

SELECT ?country ?density ?country
WHERE {
  ?country rdf:type ex:Country ;
           schema:continent "Africa" ;
           ex:density ?density .
  FILTER(xsd:decimal(?density) > 100)
}
ORDER BY DESC(?density)

// 3. Trouver les pays d’Afrique avec une densité supérieure à 100 habitants/km²
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
